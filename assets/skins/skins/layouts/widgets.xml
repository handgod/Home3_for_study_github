<?xml version="1.0"?>

<Layouts>

  <Rules>
    <PlatformCondition Platform="tablets">
      <ScreenCondition Width="1024" Height="600">
        <UseLayout Name="tablets_land_small"/>
      </ScreenCondition>

      <ScreenCondition Width="600" Height="1024">
        <UseLayout Name="tablets_port_small"/>
      </ScreenCondition>

<!-- special case for Android 3.0 Galaxy Tab -->
      <ScreenCondition Width="1024" Height="552">
        <UseLayout Name="tablets_land_small"/>
      </ScreenCondition>
      <ScreenCondition Width="600" Height="976">
        <UseLayout Name="tablets_port_small"/>
      </ScreenCondition>
<!-- -->

      <ScreenCondition Width="800" Height="480">
        <UseLayout Name="tablets_land_small"/>
      </ScreenCondition>

      <ScreenCondition Width="480" Height="800">
        <UseLayout Name="tablets_port_small"/>
      </ScreenCondition>

      <ScreenCondition Width="800" Height="600">
        <UseLayout Name="tablets_land_small"/>
      </ScreenCondition>

      <ScreenCondition Width="600" Height="800">
        <UseLayout Name="tablets_port_small"/>
      </ScreenCondition>

      <ScreenCondition Orientation="landscape">
        <UseLayout Name="tablets_land"/>
      </ScreenCondition>
      <Default>
        <UseLayout Name="tablets_port"/>
      </Default>
    </PlatformCondition>
    
    <ScreenCondition Width="240" Height="320">
      <UseLayout Name="240x320"/>
    </ScreenCondition>

    <ScreenCondition Width="320" Height="240">
      <UseLayout Name="320x240"/>
    </ScreenCondition>

    <ScreenCondition Width="360" Height="640">
      <UseLayout Name="360x640"/>
    </ScreenCondition>

    <ScreenCondition Width="320" Height="480">
      <UseLayout Name="320x480"/>
    </ScreenCondition>

    <ScreenCondition Width="480" Height="320">
      <UseLayout Name="480x320"/>
    </ScreenCondition>

    <ScreenCondition Width="400" Height="240">
      <UseLayout Name="400x240"/>
    </ScreenCondition>


    <Default>
      <ScreenCondition Orientation="landscape">
        <UseLayout Name="400x240"/>
      </ScreenCondition>
      <Default>
        <UseLayout Name="240x400"/>
      </Default>
    </Default>
  </Rules>

