<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.NoShadow.NoTitleBar" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="service_title">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:layout_marginBottom">6.0dip</item>
        <item name="android:shadowColor">#ff333333</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="all_services_title">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:shadowColor">#ff333333</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="AlertDialogCustom" parent="@android:style/Theme.Dialog">
        <item name="android:textSize">5.0sp</item>
    </style>
    <style name="WeatherParameter" />
