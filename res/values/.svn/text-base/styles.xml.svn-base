        <item name="title">@string/weather_parameter_press_title</item>
        <item name="unitsTitle">@string/weather_parameter_press_units_title</item>
        <item name="units">@array/weather_press_unit_titles</item>
        <item name="unitsValues">@array/weather_press_unit_values</item>
        <item name="formats">@array/weather_press_formats</item>
        <item name="rangeFormats">@array/weather_press_range_formats</item>
        <item name="smallIcon">@drawable/weather_pressure</item>
        <item name="largeIcon">@drawable/weather_large_pressure</item>
        <item name="initialUnits">@string/weather_pressure_default_units</item>
    </style>
    <style name="WeatherParameter.Temperature" parent="@style/WeatherParameter">
